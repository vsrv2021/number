CXX=g++
CXXFLAGS=-I. -lstdc++ -o3


# =============================================================================================================
#
# BUILD
#
# =============================================================================================================

number:
	@echo " "
	@echo "------------------------------------------------------------"
	@echo "BEAUTIFUL NUMBER CONFIGURATION: BUILD "
	@echo "------------------------------------------------------------"
	@echo " "
	
	make Number


# =============================================================================================================
#
# BUILD BEAUTIFUL NUMBER
#
# =============================================================================================================

Number: Number.cpp main.cpp
	$(CXX) $(CXXFLAGS) -o main Number.cpp main.cpp  